
CC = gcc
FLAGS = 

all: bfunc.o test

ofiles: bfunc.o ../lists/blist_double.o

bfunc.o: ../func/bfunc.c
	$(CC) $(FLAGS) -c ../func/bfunc.c -o ../func/bfunc.o

../lists/blist_double.o: ../lists/blist_double.c
	$(CC) $(FLAGS) -c ../lists/blist_double.c -o ../lists/blist_double.o

test: ../func/bfunc.o ../lists/blist_double.o test_func.c
	$(CC) $(FLAGS)  ../func/bfunc.o ../lists/blist_double.o test_func.c -o test
	@./test

clean: 
	-rm -rf test
	-rm -rf *.o

